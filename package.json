{
  "name": "ayx",
  "displayName": "ayx-vs-code",
  "description": "A visual studio code extension for Alteryx bulding block devellopment",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:ayxWorkspaces",
		"onView:testView",
    "onCommand:ayx.helloWorld"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
				"command": "ayxWorkspaces.refreshWorkspace",
				"title": "Refresh",
				"icon": {
					"light": "resources/icons/light/refresh.svg",
					"dark": "resources/icons/dark/refresh.svg"
				}
			},
      {
				"command": "ayxWorkspaces.configureWorkspace",
				"title": "Configure",
				"icon": {
					"light": "resources/icons/light/gear.svg",
					"dark": "resources/icons/dark/gear.svg"
				}
			},
      {
				"command": "ayxWorkspaces.infoWorkspace",
				"title": "Info",
        "icon": {
					"light": "resources/icons/light/info.svg",
					"dark": "resources/icons/dark/info.svg"
				}
			},
			{
				"command": "ayxWorkspaces.addEntry",
				"title": "Add",
        "icon": {
					"light": "resources/icons/light/add.svg",
					"dark": "resources/icons/dark/add.svg"
				}
			},
      {
				"command": "ayxWorkspaces.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "resources/icons/light/refresh.svg",
					"dark": "resources/icons/dark/refresh.svg"
				}
			},
      {
				"command": "ayxWorkspaces.infoEntry",
				"title": "Info",
        "icon": {
					"light": "resources/icons/light/info.svg",
					"dark": "resources/icons/dark/info.svg"
				}
			},
			{
				"command": "ayxWorkspaces.editEntry",
				"title": "Edit",
				"icon": {
					"light": "resources/icons/light/edit.svg",
					"dark": "resources/icons/dark/edit.svg"
				}
			},
			{
				"command": "ayxWorkspaces.deleteEntry",
				"title": "Delete"
			},
			{
				"command": "testView.reveal",
				"title": "Test View: Reveal"
			},
			{
				"command": "testView.changeTitle",
				"title": "Test View: Change Title"
			}
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ayx-explorer",
          "title": "Alteryx",
          "icon": "resources/media/alteryx_icon_white_256.png"
        }
      ]
    },
    "views": {
      "ayx-explorer": [
        {
          "id": "ayxWorkspaces",
          "name": "Alteryx SDK Workspace",
          "contextualTitle": "AYX Explorer"
        },
        {
          "id": "testView",
          "name": "Test"
        }
      ]
    },
    "menus": {
			"view/title": [
        {
					"command": "ayxWorkspaces.addEntry",
					"when": "view == ayxWorkspaces",
          "group": "navigation"
				},
        {
					"command": "ayxWorkspaces.configureWorkspace",
					"when": "view == ayxWorkspaces",
          "group": "navigation"
				},
				{
					"command": "ayxWorkspaces.refreshWorkspace",
					"when": "view == ayxWorkspaces",
					"group": "navigation"
				},
        {
					"command": "ayxWorkspaces.infoWorkspace",
					"when": "view == ayxWorkspaces",
          "group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "ayxWorkspaces.infoEntry",
					"when": "view == ayxWorkspaces && viewItem == tool",
					"group": "inline"
				},
        {
					"command": "ayxWorkspaces.editEntry",
					"when": "view == ayxWorkspaces && viewItem == tool",
					"group": "inline"
				},
				{
					"command": "ayxWorkspaces.deleteEntry",
					"when": "view == ayxWorkspaces && viewItem == tool"
				}
			]
		},
    "configuration": [
      {
        "title": "JSON Outline",
        "properties": {
          "jsonOutline.autorefresh": {
            "type": "boolean",
            "default": false
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "ts-loader": "^9.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "@vscode/test-electron": "^2.1.5"
  }
}
